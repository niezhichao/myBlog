<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nzc.business.dao.BlogDao">
        <insert id="insertOne" parameterType="com.nzc.business.entity.Blog">
            insert into blog_info (
            'blog_id',
            'title',
            'content',
            'tag_id',
            'file_id',
            'status',
            'create_time',
            'update_time',
            'is_original',
            'author',
            'articles_part',
            'blog_sort_id',
            'is_publish',
            'sort') values (
            #{blogId},
            #{title},
            #{content},
            #{tagId},
            #{fileId},
            #{status},
            #{createTime},
            #{updateTime},
            #{ifOriginal},
            #{author},
            #{articlesQuoted},
            #{blogSortedId},
            #{ifPublish},
            #{sort}
            )
        </insert>
</mapper>

